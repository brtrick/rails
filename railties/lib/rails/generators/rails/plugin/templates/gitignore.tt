/.bundle/
/doc/
/log/*.log
/pkg/
/tmp/
<% if with_dummy_app? -%>
<% if sqlite3? -%>
/<%= dummy_path %>/db/*.sqlite3
/<%= dummy_path %>/db/*.sqlite3-*
<% end -%>
/<%= dummy_path %>/log/*.log
<% if !skip_active_storage? -%>
/<%= dummy_path %>/storage/
<% else -%>
<% say_if_dependency_issue :active_storage, :active_record, "Skipping active_storage files in .gitignore because active_record is skipped." -%>
<% end -%>
/<%= dummy_path %>/tmp/
<% end -%>
